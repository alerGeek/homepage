    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@octokit/core": "^5.1.0",
    "chart.js": "^3.1.1",
    "mdb-angular-ui-kit": "^6.0.0",
    "octokit": "^3.1.2",
